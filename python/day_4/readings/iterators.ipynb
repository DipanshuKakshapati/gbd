    "class ToDoList:\n",
    "    def __init__(self, tasks=None):\n",
    "        self.tasks = tasks if tasks is not None else []\n",
    "\n",
    "    def add_task(self, task):\n",
    "        self.tasks.append(task)\n",
    "\n",
    "    def __iter__(self):\n",
    "        return self\n",
    "\n",
    "    def __next__(self):\n",
    "        return self"
